{
    "kibana": {
        "Kibana": {
            "group": "common",
            "provider": {
                "name": "systemd:kabana",
                "service": "kibana",
                "interval": "10s",
                "timeouts": [ "10s", "10s", "10s" ]
            },
            "dependencies": {
                "predecessors" : ["Elasticsearch"],
                "colocation" : ["Elasticsearch"],
                "relation" : []
            },
            "ha": {
                "mode": "active_passive",
                "location": {
                    "${ha.nodes.0}": "INFINITY",
                    "${ha.nodes.1}": "INFINITY"
                }
            }
        }
    }
}
