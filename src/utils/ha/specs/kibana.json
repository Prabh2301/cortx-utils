{
    "kibana": {
        "Kibana": {
            "group": "common",
            "provider": {
                "name": "systemd:kibana",
                "service": "kibana",
                "interval": "10s",
                "timeouts": [ "10s", "10s", "10s" ]
            },
            "dependencies": {
                "predecessors" : ["Elasticsearch"],
                "colocation" : ["Elasticsearch"],
                "relation" : []
            },
            "ha": {
                "mode": "active_passive",
                "location": {
                    "${HA_nodes_eosnode1}": "INFINITY",
                    "${HA_nodes_eosnode2}": "INFINITY"
                }
            }
        }
    }
}
